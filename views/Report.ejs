<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.3.3/jspdf.min.js"></script>
<script src="https://html2canvas.hertzen.com/dist/html2canvas.js"></script>
  </head>
  <body>
    <div class="container-fluid mt-5">
        <span  style="font-family: Verdana, Geneva, Tahoma, sans-serif;font-size: larger;background-color: darkgoldenrod;"><span style="font-family: Verdana, Geneva, Tahoma, sans-serif;">Total Count:</span></span>

      <div class="container-fluid">
        <table class="table table-hover table-white">
          <thead class="thead-dark" style="background-color:white;color: rgb(224, 98, 25);">
            
        <tr>
          <th scope="col">ID</th>
          <th scope="col">Patients Name</th>
          <th scope="col">Contact</th>
          <th scope="col">Client ID</th>
          <th scope="col">Date Visited</th>
          <th scope="col">Test(PT)</th>
          <th scope="col">B-Pressure</th>
          <th scope="col">weight</th>
          <th scope="col">Commodity</th>
          <th scope="col">FP Service</th>
          <th scope="col">Next Visit</th>
          <th scope="col">Commodity</th>
          <th scope="col">District</th>
          <th scope="col">Region</th>
        </tr>
      </thead>
      <tbody>
         <%  Report.forEach(function(data) { %> 
        <tr>
          <td><%= data.id %></td>
          <td><%= data.pdata %></td>
          <td><%= data.clientnumber %></td>
          <td><%= data.RegNo %></td>
          <td><%= data.Date %></td>
          <td><%= data.lab_test %></td>
          <td><%= data.blood %></td>
          <td><%= data.weight %></td>
          <td><%= data.commodity %></td>
          <td><%= data.serve %></td>
          <td><%= data.visit %></td>
          <td><%= data.Facility %></td>
          <td><%= data.District %></td>
          <td><%= data.Region %></td>
        </tr>
         <% }); %>
      </tbody>
    </table>
  </div>
  </div>


<script type="text/javascript" src="https://html2canvas.hertzen.com/dist/html2canvas.js"></script>
<script src="public/js/printable.js"></script>
  </body>
</html>